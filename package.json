{
    "name": "<My Game>",
    "version": "0.1.0",
    "private": true,
    "sideEffects": [
        "*.css"
    ],
    "homepage": "./",
    "dependencies": {
        "@tolokoban/react-state": "^0.6.0",
        "@tolokoban/tgd": "^2.0.48",
        "@tolokoban/type-guards": "^0.8.3",
        "build-react-routes": "^0.10.8",
        "json5": "^2.2.3",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "scripts": {
        "generate": "npm run routes",
        "routes": "build-react-routes ./src/app",
        "analyse": "webpack --profile --json > stats.json",
        "start": "webpack serve --mode=development",
        "build": "webpack --mode=production",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "format": "npx prettier --write src/",
        "lint": "eslint -c .eslintrc.yml 'src/**/*.ts' 'src/**/*.tsx'",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "eslintConfig": {},
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/react": "^18.3.24",
        "@types/react-dom": "^18.3.7",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^13.0.1",
        "css-loader": "^6.11.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-jsdoc": "^58.1.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-react": "^7.35.2",
        "eslint-plugin-react-hooks": "^5.2.0",
        "file-loader": "^6.2.0",
        "gh-pages": "^6.1.1",
        "highlight.js": "^11.11.1",
        "html-webpack-plugin": "^5.6.0",
        "jest": "^30.1.3",
        "markdown-to-jsx": "^7.7.13",
        "mini-css-extract-plugin": "^2.9.4",
        "prettier": "3.6.2",
        "raw-loader": "^4.0.2",
        "rehype-highlight": "^7.0.2",
        "rehype-highlight-code-lines": "^1.2.0",
        "remark-emoji": "^5.0.2",
        "remark-gfm": "^4.0.1",
        "remark-images": "^4.1.0",
        "save-as": "^0.1.8",
        "style-loader": "^3.3.4",
        "ts-jest": "^29.4.2",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.101.3",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2",
        "webpack-shell-plugin-next": "^2.3.3",
        "workbox-webpack-plugin": "^7.3.0",
        "yaml-loader": "^0.8.1"
    },
    "port": 1360
}